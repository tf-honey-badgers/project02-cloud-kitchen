<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.badgers.rest.cust.fcm.persistence.CustFcmMapper">

	<update id="updateFcmToken">
		UPDATE honey_badgers.customer SET
		fcmToken = #{fcmToken}
		WHERE id=#{id}
	</update>


</mapper>